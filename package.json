{
  "name": "rollup-plugin-inline-postcss",
  "version": "2.0.0",
  "description": "Rollup plugin that transforms inline styling with PostCSS",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "deploy": "./deploy.sh",
    "prepare": "cp src/package.json dist/package.json && cp README.md dist/README.md",
    "prettier": "prettier --write .",
    "test": "jest",
    "version": "./version.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steveblue/rollup-plugin-inline-postcss.git"
  },
  "keywords": [
    "rollup",
    "postcss",
    "plugin"
  ],
  "author": "Steve Belovarich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steveblue/rollup-plugin-inline-postcss/issues"
  },
  "homepage": "https://github.com/steveblue/rollup-plugin-inline-postcss#readme",
  "dependencies": {
    "findup": "^0.1.5",
    "postcss": "^8.3.5",
    "rollup": "^1.0.0",
    "rollup-pluginutils": "^2.3.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "jest": "^26.6.0",
    "json-bump": "^1.0.2",
    "postcss-csso": "^5.0.0",
    "postcss-rgb-plz": "^2.0.2",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
